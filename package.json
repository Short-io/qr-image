{
    "name": "@shortcm/qr-image",
    "version": "7.0.1",
    "description": "QR Code generator (png, svg, pdf)",
    "homepage": "https://github.com/Short-io/qr-image",
    "keywords": [
        "qrcode",
        "qr code",
        "qr",
        "png",
        "svg",
        "pdf",
        "image"
    ],
    "author": {
        "name": "Andrii Kostenko",
        "url": "http://git.io/gugu"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Short-io/qr-image.git"
    },
    "bugs": "https://github.com/Short-io/qr-image/issues",
    "type": "module",
    "main": "lib/qr.js",
    "exports": {
        "./lib/png": {
            "node": "./lib/png.js",
            "types": "./lib/png.d.ts",
            "default": "./lib/png_browser.js"
        },
        "./lib/svg": {
            "types": "./lib/svg.d.ts",
            "default": "./lib/svg.js"
        },
        "./lib/pdf": {
            "types": "./lib/pdf.d.ts",
            "default": "./lib/pdf.js"
        }
    },
    "scripts": {
        "test": "ava",
        "prepare": "tsc",
        "release": "release-it"
    },
    "size-limit": [
        {
            "path": "lib/qr.js"
        }
    ],
    "files": [
        "lib",
        "LICENSE"
    ],
    "devDependencies": {
        "@ava/typescript": "^3.0.1",
        "@size-limit/preset-small-lib": "^8.1.0",
        "@types/jsdom": "^20.0.1",
        "@types/sharp": "^0.31.0",
        "ava": "^5.0.1",
        "benchmark": "^2.1.4",
        "canvas": "^2.10.2",
        "eslint": "^8.27.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "looks-same": "^8.0.1",
        "microtime": "^3.1.1",
        "release-it": "^15.5.0",
        "size-limit": "^8.1.0",
        "@types/diff": "^5.0.2",
        "chalk": "^5.1.2",
        "diff": "^5.1.0",
        "jsdom": "^20.0.2",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "pdf-lib": "^1.17.1",
        "sharp": "^0.31.2",
        "xml-formatter": "^2.6.1"
    }
}
